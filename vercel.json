{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "build/server/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/handler" }
  ],
  "env": {
    "NPM_CONFIG_INCLUDE": "dev"
  }
}
